<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>Lab7</display-name>
    <context-param>
        <description>url</description>
        <param-name>URL1</param-name>
        <param-value>http://localhost:8180/Lab7/url1.html</param-value>
    </context-param>
    <context-param>
        <description>url</description>
        <param-name>URL2</param-name>
        <param-value>http://localhost:8180/Lab7/url2</param-value>
    </context-param>
    <servlet>
        <servlet-name>Task1Servlet</servlet-name>
        <servlet-class>lab7.Task1Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Url2Servlet</servlet-name>
        <servlet-class>lab7.Url2Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ccc</servlet-name>
        <servlet-class>lab7.Ccc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Task1Servlet</servlet-name>
        <url-pattern>/Task1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Url2Servlet</servlet-name>
        <url-pattern>/url2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ccc</servlet-name>
        <url-pattern>/Ccc</url-pattern>
    </servlet-mapping>
</web-app>